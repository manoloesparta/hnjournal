{
  "env":{
    "browser":true,
    "commonjs":true,
    "es2021":true
  },
  "extends":"eslint:recommended",
  "parserOptions":{
    "sourceType":"module",
    "ecmaVersion":12
  },
  "rules":{
    "indent":[
      "error",
      2
    ],
    "linebreak-style":[
      "error",
      "unix"
    ],
    "quotes":[
      "error",
      "single"
    ],
    "semi":[
      "error",
      "never"
    ]
  }
}